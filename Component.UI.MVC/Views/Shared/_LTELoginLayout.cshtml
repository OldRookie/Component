
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @*@Scripts.Render("~/bundles/modernizr")*@

    <!-- Bootstrap 3.3.7 -->
    <link href="~/Scripts/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Scripts/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Scripts/lib/lte/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Scripts/lib/lte/css/skins/_all-skins.css" rel="stylesheet" />

    <script src="~/Scripts/config.js?v=1.7"></script>
    <script src="~/Scripts/require.js"></script>
    @*<script src="~/Scripts/dist/main.js"></script>*@
    <script src="~/Scripts/main.js"></script>
    @{
        List<string> pemission = new List<string>();
        if (IdentityInfoHelper.CurrentIdentityInfo != null)
        {
            pemission = IdentityInfoHelper.CurrentIdentityInfo.Pemission;
        }
    }
    <script>
        require(["system.permission"], function (permission) {
            permission.setAllModuleAccessControl(@Html.Raw(pemission.ToJson()));
        });
    </script>
    <!-- jvectormap -->
</head>
<body class="hold-transition login-page">
    <div class="wrapper">
        @RenderBody()
        <div class="window-overlay" style="display:none">
            <i class="fa fa-refresh fa-spin"></i>
        </div>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
