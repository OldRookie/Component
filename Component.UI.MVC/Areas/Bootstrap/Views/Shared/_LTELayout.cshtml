<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @*@Scripts.Render("~/bundles/modernizr")*@

    <!-- Bootstrap 3.3.7 -->
    <link href="~/Scripts/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Scripts/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Scripts/lib/lte/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Scripts/lib/lte/css/skins/_all-skins.css" rel="stylesheet" />
    @*<script src="~/Scripts/jquery-3.3.1.js"></script>*@
    @*<script src="~/Scripts/lib/lte/js/adminlte.js"></script>*@
    @*<script src="~/Scripts/lib/bootstrap/dist/js/bootstrap.js"></script>*@

    <script src="~/Scripts/config.js"></script>
    <script data-main="/Scripts/main.js" src="~/Scripts/require.js"></script>
    @*<script src="~/Scripts/dist/main.js"></script>*@
    @*<script src="~/Scripts/main.js"></script>*@
    <!-- jvectormap -->
</head>
<body class="skin-blue sidebar-mini">
    <div class="wrapper">
        @Html.Partial("_LTEHeader")
        <!-- Left side column. contains the logo and sidebar -->

        @Html.Partial("_LTEMenu")
        @RenderBody()
        @Html.Partial("_LTEFooter")
    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)

</body>
</html>
